    <owl:equivalentClass rdf:resource="https://schema.org/AssessAction"/>
    <rdfs:subClassOf rdf:resource="https://www.lib.montana.edu/ontology/biosciences#action"/>
    <rdfs:label>AssessAction</rdfs:label>
